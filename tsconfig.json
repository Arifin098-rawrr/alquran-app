{
  "compilerOptions": {
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/app/page.jsx", "src/app/components/HeroSection.jsx", "src/app/components/KegiatanSection.jsx", "src/app/components/AboutSection.jsx", "src/app/components/PrayerTimeSection.jsx", "src/app/components/QuranSection.jsx", "src/components/Footer.jsx", "src/components/WaktuShalat.jsx", "src/app/layout.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
